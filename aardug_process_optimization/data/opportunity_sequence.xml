<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="sequence_opportunity" model="ir.sequence">
        <field name="name">Opportunity code</field>
        <field name="code">crm.lead</field>
        <field eval="4" name="padding"/>
        <field name="prefix">k</field>
         <field name="padding">3</field>
    </record>
    <record id="sequence_task" model="ir.sequence">
        <field name="name">Task code</field>
        <field name="code">project.task</field>
        <field eval="4" name="padding" />
        <field name="prefix">T</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="company_id"/>
    </record>
</odoo>
